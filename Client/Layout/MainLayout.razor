@inherits LayoutComponentBase

<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
</head>

<div class="page" style="font-family: Work Sans, Arial, sans-serif; height:none !important">

	<main>
		<nav class="navbar sticky-top shadow-sm" style="background:#ffffff; height: 6.5em;">
			<div class="container-fluid justify-content-start">
				<button class="navbar-toggler my-navbar-button" type="button" data-bs-toggle="offcanvas" data-bs-target="#mainSidebar" aria-controls="mainSidebar" aria-expanded="false" aria-label="Toggle navigation">
					<span class="navbar-toggler-icon"></span>
				</button>
				<span class="mynavbar-brand-left">Healt</span>
				<span class="mynavbar-brand-right">hive.</span>	
				<div class="mynavbar-profile-div">
					<i class="bi bi-person-circle mynavbar-profile-div-icon"></i>
					<div class="btn-group mynavbar-profile-div-menu">
						<button class="btn dropdown-toggle mynavbar-profile-div-menu-button" type="button" data-bs-toggle="dropdown" aria-expanded="false">
							BLOO
						</button>
						<ul class="dropdown-menu mynavbar-profile-div-menu-popup">
							<li><a class="dropdown-item mynavbar-profile-div-menu-popup-text" href="#">PROFILE</a></li>
							<li><a class="dropdown-item mynavbar-profile-div-menu-popup-text" href="#">SETTINGS</a></li>
							<li><hr class="dropdown-divider"></li>
							<li><a class="dropdown-item mynavbar-profile-div-menu-popup-text" href="#">PRIVACY POLICY</a></li>
							<li><a class="dropdown-item mynavbar-profile-div-menu-popup-text" href="#">HELP</a></li>
							<li><hr class="dropdown-divider"></li>
							<li><a class="dropdown-item mynavbar-profile-div-menu-popup-text" href="#">SIGN OUT</a></li>
						</ul>
					</div>
				</div>				
				<div class="offcanvas offcanvas-start my-offcanvas" tabindex="-1" id="mainSidebar" aria-labelledby="offcanvasNavbarLabel">
                    <div class="offcanvas-header my-offcanvas-div-button">                        
                        <button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="Close" ></button>
                    </div>
					<div>
						<button class="accordion-button my-accordion-button" type="button">
							<i class="bi bi-house-heart" style="font-size:1.2em;  margin-right: 1em;"></i>
							&nbsp;HOME
						</button>
					</div>
					<div class="accordion accordion-flush ynav-accordion" id="accordionFlushExample">
						<div class="accordion-item">
							<h2 class="accordion-header">
								<button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseOne" aria-expanded="false" aria-controls="flush-collapseOne">
									<i class="bi bi-clipboard2-data" style="font-size:1.2em; margin-right: 1em;"></i>
									&nbsp;MY DATA
								</button>
							</h2>
							<div id="flush-collapseOne" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
								<div class="accordion-body">
									<a class="mynav-menu-link" href="#">ADD NEW</a>
								</div>
							</div>
							<div id="flush-collapseOne" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
								<div class="accordion-body">
									<a class="mynav-menu-link" href="#">MODIFY EXISTING</a>
								</div>
							</div>
							<div id="flush-collapseOne" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
								<div class="accordion-body">
									<a class="mynav-menu-link" href="#">VIEW</a>
								</div>
							</div>
						</div>
						<div class="accordion-item">
							<h2 class="accordion-header">
								<button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseTwo" aria-expanded="false" aria-controls="flush-collapseTwo">
									<i class="bi bi-people" style="font-size:1.2em;  margin-right: 1em;"></i>
									&nbsp;MY FAMILY
								</button>
							</h2>
							<div id="flush-collapseTwo" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
								<div class="accordion-body">
									<a class="mynav-menu-link" href="#">ADD NEW MEMBER</a>
								</div>
							</div>
							<div id="flush-collapseTwo" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
								<div class="accordion-body">
									<a class="mynav-menu-link" href="#">UPDATE MEMBER DETAILS</a>
								</div>
							</div>
							<div id="flush-collapseTwo" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
								<div class="accordion-body">
									<a class="mynav-menu-link" href="#">VIEW MEMBERS</a>
								</div>
							</div>
						</div>
						<div class="accordion-item">
							<h2 class="accordion-header">
								<button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseThree" aria-expanded="false" aria-controls="flush-collapseThree">
									<i class="bi bi-gear" style="font-size:1.2em;  margin-right: 1em;"></i>
									&nbsp;SETTINGS
								</button>
							</h2>
							<div id="flush-collapseThree" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
								<div class="accordion-body">
									<a class="mynav-menu-link" href="#">VIEW LOGS</a>
								</div>
							</div>
						</div>
					</div>
                </div>
			</div>
		</nav>		
		<div class="container-fluid">
			@Body
		</div>
	</main>
</div>

